      XY[1]       =   mul(XY[1],matViewProjectionInverse);
      XY[2]       =   mul(XY[2],matViewProjectionInverse);
      XY[3]       =   mul(XY[3],matViewProjectionInverse);
      
      XY[0].xyz   /=   XY[0].w;
      XY[1].xyz   /=   XY[1].w;
      XY[2].xyz   /=   XY[2].w;
      XY[3].xyz   /=   XY[3].w;
      
      
      XY[0].xyz   -=   oldPos.xyz;
      XY[1].xyz   -=   oldPos.xyz;
      XY[2].xyz   -=   oldPos.xyz;
      XY[3].xyz   -=   oldPos.xyz;
      
      float4 d;
      d.x   =   dot(viewDir.xyz,normalize(XY[0].xyz));
      d.y   =   dot(viewDir.xyz,normalize(XY[1].xyz));
      d.z   =   dot(viewDir.xyz,normalize(XY[2].xyz));
      d.w   =   dot(viewDir.xyz,normalize(XY[3].xyz));
      oldocc+=dot(d,1);
      occ += dot(acos(d),1);
   }
   
  // vTotalDir   =   normalize(normalize(vTotalDir)-viewDir);
   //fVEAngle   =   acos(dot(oldNor,vTotalDir))/3.14159265f;
   
  
