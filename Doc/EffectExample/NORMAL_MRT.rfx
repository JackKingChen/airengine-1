     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 fvAmbient;
float4 fvSpecular;
float4 fvDiffuse;
float fSpecularPower;

sampler2D baseMap;
sampler2D bumpMap;

struct VS_OUTPUT 
{
   float4 Position :        POSITION0;
   float2 Texcoord :        TEXCOORD0;
   float4 fvProj   :   TEXCOORD1;
   float3 fvNormal :   TEXCOORD2;
   float3 fvTangent:   TEXCOORD3;
   
};
struct PS_OUTPUT{
   float4   depth:COLOR0;
   float4   nor:COLOR1;
   float4   diff:COLOR2;
};

PS_OUTPUT ps_main( VS_OUTPUT Input )
{      
   PS_OUTPUT o;
   

   float3 fvNormal         = normalize( ( tex2D( bumpMap, Input.Texcoord ).xyz * 2.0f ) - 1.0f );

   float3   vNormal      =   normalize(Input.fvNormal.xyz);
   float3   vTangent     =   normalize(Input.fvTangent.xyz);
   float3   fvBinormal   =   cross(fvNormal,vTangent);
   
   float3x3 rot   =   float3x3(
                        vTangent,fvBinormal,vNormal
                        );
   float3 finalnormal     =   mul(fvNormal*float3(1,1,1),rot);
   o.depth   =   Input.fvProj.z/Input.fvProj.w;
   o.nor      =   float4(finalnormal*0.5+0.5,0);
   o.diff      =    tex2D( baseMap, Input.Texcoord );
   
   return o;
      
}



]]></RmShaderCode>
