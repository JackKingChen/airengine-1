     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float3 fvLightPosition;
float3 fvEyePosition;
float4x4 matView;
float4x4 matViewProjection;

struct VS_INPUT 
{
   float4 Position : POSITION0;
   float2 Texcoord : TEXCOORD0;
   float3 Normal :   NORMAL0;
   float3 Binormal : BINORMAL0;
   float3 Tangent :  TANGENT0;
   
};

struct VS_OUTPUT 
{
   float4 Position :        POSITION0;
   float2 Texcoord :        TEXCOORD0;
   float4 fvProj   :   TEXCOORD1;
   float3 fvNormal :   TEXCOORD2;
   float3 fvTangent:   TEXCOORD3;
   
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position         = mul( float4(Input.Position.xyz*0.01,1), matViewProjection );
   Output.Texcoord         = Input.Texcoord;
   
   Output.fvProj = Output.Position;//, matView );
   Output.fvNormal         = Input.Normal;//
   Output.fvTangent        = Input.Tangent;//, matView );
      
   
   return( Output );
   
}


]]></RmShaderCode>
