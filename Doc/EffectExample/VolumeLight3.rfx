     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 vFogColorNL;
float fFogMax;
float4 vAmbient;
float4 vFogColor;
sampler2D Texture2;
float4x4 matShadowVP;
sampler2D Texture1;
float4 vCamPos;
float4 vLightPos;
float2 fInvViewport;
sampler2D baseMap;
float fFogdensity;
sampler2D Texture3;
float4x4 matShadowVPInv;
float fLightPower;

struct PS_INPUT 
{
   
   float2 Texcoord : TEXCOORD0;
   float3  Normal:TEXCOORD1;
   float4   ProjPos:TEXCOORD2;
   float4   WorldPos:TEXCOORD3;
};
struct PS_OUTPUT{
   float4   Depth   :   COLOR0;
};

float2   XY_UV(float2 xy){
   return   xy*float2(0.5f,-0.5f)+float2(0.5f,0.5f);
}

#define   SAMPLER_NUMBER   64
float4 ps_main( PS_INPUT Input ):COLOR
{
   float2   hPixel     =   0.5f*fInvViewport;
   float4   xy         =   Input.ProjPos/Input.ProjPos.w;
   float2   texcoord   =   XY_UV(xy.xy);
   float2   uv         =   texcoord+hPixel;
   
   float   ao         =   tex2D(Texture3,texcoord);
   
   float2   UV2X2      =   (uv*0.5f)/fInvViewport;
   float    fStep      =   tex2D(Texture2,UV2X2).x;
   float4   baseColor  =   tex2D(baseMap,Input.Texcoord);
   
   float3   vDir   =   (Input.WorldPos.xyz-vCamPos.xyz)/SAMPLER_NUMBER;
   float3   vStart   =   vCamPos.xyz+vDir*(1-fStep);
   float   fMaxLength   =   length(vDir*SAMPLER_NUMBER);
   float   fAlpha          =   0;
   float   fStepValue      =   saturate(fMaxLength/fFogdensity)/SAMPLER_NUMBER;
   float4   FinalFogColor   =   0;
   
   float4   OldShadow   =   mul(float4(Input.WorldPos.xyz,1),matShadowVP);
   OldShadow/=OldShadow.w;
   float2   OldShadowUV   =    XY_UV(OldShadow.xy)+hPixel;
   float    Oldshadowz   =   tex2D(Texture1,OldShadowUV).x;
   float   fShadow      =  (OldShadow.z <(Oldshadowz+0.0001))&&(OldShadow.z>0);
   
   #if 1
   
   for(int i=0;i<SAMPLER_NUMBER;i++){
      float3   newPos   =  vStart+vDir*i;
      float4   ShadowProj   =   mul(float4(newPos,1),matShadowVP);
      ShadowProj/=ShadowProj.w;
      float2   shadowuv   =   XY_UV(ShadowProj.xy)+hPixel;
      float    shadowz   =   tex2D(Texture1,shadowuv).x;
      
      float   fCamDis      =   length(newPos-vCamPos.xyz);
      
      float    lweight   =   saturate((fFogdensity/distance(newPos.xyz,vLightPos.xyz)));
      float   weight   =   saturate((fFogdensity/fCamDis));
      
      if(ShadowProj.z < shadowz+0.0001){
         
         //FinalFogColor   +=   (vFogColor*lweight*vFogColor.w+vFogColorNL*vFogColorNL.w*0.1)*weight*(1-fAlpha);
         fAlpha         +=   lweight;
      }
   }
      //float   fLightDis   =   distance(Input.WorldPos.xyz,vLightPos.xyz);
   //float   fLightScale   =   saturate(fFogdensity/fLightDis);
   //float   NLFogWeight   =   saturate(1-fFogdensity/fMaxLength);
   
   float4   diffColor   =   baseColor*(ao*vAmbient+fShadow*vFogColor);
   
   float maxDis         =   length((Input.WorldPos.xyz-vCamPos.xyz));
   

   return diffColor+vFogColor*(fAlpha/SAMPLER_NUMBER)*maxDis/fFogMax;
   
   #else
   float3   LightCamDir   =   vCamPos.xyz-vLightPos.xyz;
   float3   LookDir       =   Input.WorldPos.xyz-vLightPos.xyz;
   float3   X               =   normalize(LookDir);
   float3   Z               =   normalize(cross(normalize(LightCamDir),X));
   float3   Y               =   normalize(cross(X,Z));
   
   float4   CamShadowPos     =   mul(float4(vCamPos.xyz,1),matShadowVP);
   CamShadowPos/=CamShadowPos.w;
   float4   WC              =   mul(float4(OldShadow.xy,CamShadowPos.z,1),matShadowVPInv);
   WC/=WC.w;
   
   float2   B               =   float2(length(LookDir),0);
   float2   A               =   float2(dot(LightCamDir,X),dot(LightCamDir,Y));
   float2   C               =   float2(dot(WC.xyz,X),dot(WC.xyz,Y));
   
   float   BA               =   distance(B,A);
   
   float2   AUV               =   XY_UV(CamShadowPos.xy);
   float2   BUV               =   OldShadowUV;


   float   kBA   =   A.y/(A.x-B.x);
   float   dBA   =   A.y/(1-A.x/B.x);
   
   float2 UVDir   =   BUV-AUV;
   float   uvlen   =   length(UVDir);
   UVDir         =   normalize(UVDir)*(fInvViewport);
   
   float alpha =0;
   
   for(int i=0;i<SAMPLER_NUMBER;i++){
        
         float   w   =   (i+0.25)/SAMPLER_NUMBER;
         float2   D   =   lerp(A,C,w);//*0.5f;
   
         float kDO   =   D.y/D.x;
         float2   E=0;
         E.x   =   dBA/(kDO-kBA);
         E.y   =   kDO*E.x;
         
         float3   worldpos   =   lerp(vCamPos.xyz,Input.WorldPos.xyz,distance(E,A)/BA);
         float4   shadowpos   =   mul(float4(worldpos,1),matShadowVP);
         shadowpos/=shadowpos.w;
         float2   shadowuv   = lerp(AUV,BUV,w);
         float z            =   tex2D(Texture1,shadowuv).x;
         if(shadowpos.z<z){
            alpha+=1.0f;
         }
    }
   float4   diffColor   =   baseColor*(ao*vAmbient+fShadow*vFogColor);
   return   alpha/SAMPLER_NUMBER;//diffColor+vFogColor*(fAlpha/SAMPLER_NUMBER)*distance(AUV,BUV)*fLightPower;
   #endif
   

   
}



]]></RmShaderCode>
