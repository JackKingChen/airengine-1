float2 AOParam;

float4   temp   =   float4(2,-2,-1,1);

float2 GetOffset(float4 off,float4 option){
   return (off.xy*option.x+off.yx*(1-option.x))*option.y;
};

float2 fInvViewport;
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
   float2   uv=   texCoord+0.5f*fInvViewport;
   float2   xy=   uv*temp.xy+temp.zw;
   float4   opt   =  tex2D(Texture3,uv*0.5f/fInvViewport);
   opt.y = opt.y*temp.x-temp.w;
  
   float4   oldPos=   temp.w;
   oldPos.xy      =   xy;
   oldPos.z       =   tex2D(Texture1,uv).x;
   oldPos         =   mul(oldPos,matViewProjectionInverse);
   oldPos.xyz     = oldPos.xyz/oldPos.w;
   float3   oldNor=   tex2D(Texture0,uv).xyz*temp.x-temp.w;
   //normalize(oldNor);
   //oldNor         =   normalize(mul(oldNor,(float3x3)matViewProjectionInverse));

   
   float4   tempUV;
   float2   tempXY;

   float4   tempPos   =   temp.w;
   //float3   tempDir;
   
   float2 occ=SAMPLER_NUM;
   tempUV.zw   =   downoffset.yy*AOParam.y;
   
   float2 UV[4];
   float4 XY[4];

   //[loop]
   for(int i=0;i<SAMPLER_NUM;i+=4){
      UV[0]     =   GetOffset(offset[i]  ,opt)*tempUV.zw+uv;
      UV[1]     =   GetOffset(offset[i+1],opt)*tempUV.zw+uv;
      UV[2]     =   GetOffset(offset[i+2],opt)*tempUV.zw+uv;
      UV[3]     =   GetOffset(offset[i+3],opt)*tempUV.zw+uv;

      XY[0].xy     =   UV[0]*temp.xy+   temp.zw;
      XY[1].xy     =   UV[1]*temp.xy+   temp.zw;
      XY[2].xy     =   UV[2]*temp.xy+   temp.zw;
      XY[3].xy     =   UV[3]*temp.xy+   temp.zw;
      
      XY[0].z      =   tex2D(Texture1,UV[0]).x;
      XY[1].z      =   tex2D(Texture1,UV[1]).x;
      XY[2].z      =   tex2D(Texture1,UV[2]).x;
      XY[3].z      =   tex2D(Texture1,UV[3]).x;
      
      XY[0].w      =   1;
      XY[1].w      =   1;
      XY[2].w      =   1;
      XY[3].w      =   1;

      XY[0]       =   mul(XY[0],matViewProjectionInverse);
      XY[1]       =   mul(XY[1],matViewProjectionInverse);
      XY[2]       =   mul(XY[2],matViewProjectionInverse);
      XY[3]       =   mul(XY[3],matViewProjectionInverse);
      
      XY[0].xyz   /=   XY[0].w;
      XY[1].xyz   /=   XY[1].w;
      XY[2].xyz   /=   XY[2].w;
      XY[3].xyz   /=   XY[3].w;
      
      
      XY[0].xyz   -=   oldPos.xyz;
      XY[1].xyz   -=   oldPos.xyz;
      XY[2].xyz   -=   oldPos.xyz;
      XY[3].xyz   -=   oldPos.xyz;
      
      float4 l;
      
      l.x      =   length(XY[0].xyz);
      l.y      =   length(XY[1].xyz);
      l.z      =   length(XY[2].xyz);
      l.w      =   length(XY[3].xyz);
      
      float4 o =   temp.w-l/AOParam.x;
      
      normalize(XY[0].xyz);//    /=   l.x;
      normalize(XY[1].xyz);//    /=   l.y;
      normalize(XY[2].xyz);//    /=   l.z;
      normalize(XY[3].xyz);//    /=   l.w;
      
      float4 d;
      d.x   =   dot(oldNor.xyz,XY[0].xyz);
      d.y   =   dot(oldNor.xyz,XY[1].xyz);
      d.z   =   dot(oldNor.xyz,XY[2].xyz);
      d.w   =   dot(oldNor.xyz,XY[3].xyz);
      


      occ.x-=dot(saturate(d*0.5+0.5),saturate(o*0.5+0.5));
      //co+=length(tempDir);
   }

  
   
   return occ.x/SAMPLER_NUM;
}

