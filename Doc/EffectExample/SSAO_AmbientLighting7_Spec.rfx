     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
struct VS_OUTPUT
{
   float4 pos       : POSITION0;
   float2 texCoord  : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT o = (VS_OUTPUT) 0;

   inPos.xy = sign( inPos.xy);
   o.pos = float4( inPos.xy, 1.0f, 1.0f);

   // get into range [0,1]
   o.texCoord = (float2(o.pos.x, -o.pos.y) + 1.0f)/2.0f;
   return o;
}
]]></RmShaderCode></RmHLSLShader>
     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="1"><RmShaderCode TYPE="TEXT"><![CDATA[
float fReflAngle;
sampler2D Texture7;
sampler2D Texture6;
sampler2D Texture4;
float fNor;
sampler2D Texture0;
sampler2D Texture1;
sampler2D Texture2;
sampler2D Texture3;
sampler2D Texture5;
#define SAMPLER_NUM 2
#define STEP_NUM    8

float4x4 matViewProjectionInverse;



float2 invViewport;
float4 vViewPosition;


float4 OccSize;

float4   temp   =   float4(2,-2,-1,1);

float2 GetOffset(float4 off,float4 option){
   return (off.xy*option.x+float2(1,-1)*off.yx*(1-option.x))*option.y;
};

float2 GetOffset2(float2 initxy,int   i,float rotoffset){
   //float2 initxy   =   float2(0,1);
   float fsin,fcos;
   sincos((i*0.25f/SAMPLER_NUM+rotoffset)*2.0f*3.1415926f,fsin,fcos);
   float2x2 rot   =   float2x2(fcos,-fsin,fsin,fcos);
   return mul(initxy,rot)*float2(1,-1);
};

float4   GenPos(float2 fuv,float iLod){
      float4 XY;
      XY.xy     =   fuv*temp.xy+   temp.zw;     
      XY.z      =   tex2Dlod(Texture1,float4(fuv,0,iLod)).x;
      XY.w      =   1;
      XY       =   mul(XY,matViewProjectionInverse);
      XY      /=   XY.w;
      
      return XY;
};

float fNormal;
float4 ps_main( float2 texCoord  : TEXCOORD0,int2 ScreenPos:VPOS ) : COLOR
{
   float2 halfPixel   =   0.5f*invViewport;
   float2   uv=   texCoord+halfPixel;
   float2   xy=   uv*temp.xy+temp.zw;
   float4   noisep     =   tex2D(Texture5,uv/invViewport/4);
   
   float   rotoffset   =  tex2D(Texture3,uv/invViewport/2).x+0.125f;
   //noisep   =   noisep.x*float4(1,2,4,8);

   float4   oldPos;
   oldPos.xy      =   xy;
   oldPos.z       =   tex2D(Texture1,uv).x;
   oldPos.w      =   1;
   
   oldPos         =   mul(oldPos,matViewProjectionInverse);
   
   oldPos/=oldPos.w;
   /*
   oldPos.xyz   =   normalize(oldPos.xyw)*oldPos.z;
   */
   float3   N=   tex2D(Texture0,uv).xyz;
   N=N*2-1;
   
   //float fDot   =   dot(oldNor,float(0,0,1));
   float2 xydir   =   normalize(N.xy);
   
   float3 viewDir   =   -normalize(oldPos.xyz);
   
   float3   V         =   viewDir;         
   float3   R         =   reflect(-V,N);
   float    fDotVR    =   dot(V,R);
   float    fAngleVR  =   acos(fDotVR);
   float2   uvangle   =   float2(fDotVR*0.5+0.5,fReflAngle);
   float   fRealReflAngle   =   fReflAngle*3.1415926f;
   
   float   coneangle   =   tex2D(Texture6,uvangle).x*0.5;
   float weight2   =   tex2D(Texture7,float2(rotoffset,0.0f)).y*3.14159265;
   
   float2 weightangle   =   float2(fAngleVR+weight2,fAngleVR-weight2);
   if(weightangle.x   > 3.1415926f){
      weightangle.x   =   3.1415926f;
   }
   if(weightangle.y   < 0.0f){
      weightangle.y   =   0.0f;
   }
   float2 fDotWeight   =   cos(weightangle);//*0.5+0.5;

   
   float4   tempUV;
   float2   tempXY;

   float4   tempPos   =   temp.w;
   //float3   tempDir;
   
   float4 occ=0;
   tempUV.zw   =   invViewport*OccSize.y;


   float offset   =   lerp(-coneangle,coneangle,rotoffset+(noisep.y)*0.25f/SAMPLER_NUM);
   //int i   =   0;
   //[loop]
   for(int i=0;i<SAMPLER_NUM;i++)
   {
      float2   tempuv   =   GetOffset2(xydir,i    ,offset)*tempUV.zw;
      
      float ffinaldot   =   -1;
      [loop]
      for(int j=0;j<STEP_NUM;j++){
         float2 UV     =   tempuv*noisep.x*(j+1)   +texCoord;
         float4 XY     =   GenPos(UV,j);
         XY.xyz   -=   oldPos.xyz;
         float fdot   =   dot(viewDir.xyz,normalize(XY.xyz));
         if(fdot   >   ffinaldot)
            ffinaldot   =   fdot;
      }
      
      if(ffinaldot>fDotWeight.x){
         fDotWeight.x   =   ffinaldot;
      }
     
      //float fmin   =   dot(fdot,0.25);//
      float   x   =   ffinaldot*0.5+0.5;
      float   y   =   (i*0.25)/SAMPLER_NUM+offset;//+0.5/64.0f;
      
      float2 satuv         =   float2(fDotWeight.x,y);
      float4 currentEnv    =   tex2D(Texture4,satuv);
      float4 maxEnv        =   tex2D(Texture4,float2(fDotWeight.y,y));

      occ += max(maxEnv-currentEnv,0);
      //co+=length(tempDir);
   }

  
  //float   fWeight   =   lerp(dot(viewDir,oldNor)*0.25+0.75f,1,fNor);
   float4 fAO   =   ((occ/SAMPLER_NUM));//*fWeight;
   /*
   float dd = dot(viewDir.xyz,oldNor.xyz);
   float2 uv2   =   0;
   uv2.x   =   1-(dd*0.5+0.5);
   float2 uvdir   =   normalize(oldNor.xy);
   uv2.y   =   dot(float2(0,-1),uvdir)*0.25+0.25;
   if(uvdir.x<0){
      uv2.y   =   1.0f-uv2.y;
   }
   return tex2D(Texture4,uv2);
   */
   //return float4(GetOffset2(1    ,rotoffset)*0.5+0.5,0,0);
   return fAO/cos(fReflAngle);//pow(acos(-fAO)/3.14f,0.5f);///4;
}

]]></RmShaderCode>
