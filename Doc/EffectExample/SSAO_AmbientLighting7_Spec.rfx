     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D Texture5;
float fReflAngle;
sampler2D Texture4;
sampler2D Texture3;
sampler2D Texture0;
sampler2D Texture1;
sampler2D Texture2;


float2 invViewport;
float4 vViewPosition1;
float  fWalkStep;
float  fAngle;
float4 WaterColor;
float4x4 matProjection;
float4x4 matViewProjectionInverse;

struct PS_INPUT 
{
   float2 TexCoord: TEXCOORD0;
   float3 Normal:   TEXCOORD1;
   float3 WorldPos: TEXCOORD2;
   float3 Tangent:  TEXCOORD3;
   float4 Proj:TEXCOORD4;
};
float fTime0_1;
float fHeight;

float2 XY_UV(float2 xy){
   return   xy*float2(0.5,-0.5)+float2(0.5,0.5);
};
float2 UV_XY(float2 uv){
   return   uv*float2(2,-2)+float2(-1,1);
};
float fOccWeight;




float fn;

float4 ps_main( float2 texCoord  : TEXCOORD0 ):COLOR
{
   float2 halfPixel   =   0.5f*invViewport;
   float2   uv=   texCoord+halfPixel;
   float2   xy=   UV_XY(uv);

  
   float4   oldPos;
   oldPos.xy      =   xy;
   oldPos.z       =   tex2Dlod(Texture0,float4(uv,0,0)).x;
   oldPos.w      =   1;
   oldPos         =   mul(oldPos,matViewProjectionInverse);
   oldPos/=oldPos.w;
   
   float4   vNor   =   tex2Dlod(Texture3,float4(uv,0,0));

   float3 nor =vNor.xyz*2-1;
   
   float3 viewdir   =   normalize(oldPos.xyz);
   
   float3   wrefl   =   normalize(reflect(viewdir,nor));
   
   float3   newDirWolrdPos     =  oldPos.xyz+wrefl*2;
   
   float4   newDirProjPos      =   mul(float4(newDirWolrdPos,1),matProjection);
   newDirProjPos/=newDirProjPos.w;
   
   float2   newDirProjUV         =   XY_UV(newDirProjPos.xy);
   
   float2   UVDir                =   normalize(newDirProjUV   -   uv)*fWalkStep;
   
   
   
   float2   randomuv         =   (uv)/invViewport/4.0f;
   
   float4   offset               =   tex2D(Texture2,randomuv);//tex2D(Texture2,randomuv).x+1.5f;
   offset.x+=1.0f;
   float rotangle            =   tex2Dlod(Texture5,float4(uv*2.0f,0,0)).x+1;
   
   //UVDir   +=   fHeight*UVDir.yx*float2(1,-1)*(rotangle*2-1);
   

   float4   retColor   =   0;
   
   
   float    fDotAngle   =   dot(-viewdir,wrefl)*0.5+0.5;
   
   
   
   float2 retUV   =   float2(-1,-1);
   
   float fDotVR   =   1.0f;;

   [loop]
   for(int i=0;i<32;i++){
   
      float2   newUV   =   uv   +  UVDir*(i+rotangle);
      float4   newProjPos;
      newProjPos.xy   =   UV_XY(newUV);
      newProjPos.z      =   tex2Dlod(Texture0,float4(newUV,0,0)).x;
      newProjPos.w      =   1;
      
      newProjPos   =   mul(newProjPos,matViewProjectionInverse);
      newProjPos/=newProjPos.w;
      
      float3   newDir   =   normalize(newProjPos.xyz-oldPos.xyz);

      float   d          =   dot(newDir,wrefl)>0.999f;
      
      float fVR   =   dot(viewdir,newDir);
      if(fVR   <   fDotVR){
         fDotVR   =   fVR;
      }
      
      if(d>0.5   ){

            //retUV      =  newUV;
 
            //break;
         
      } 
   }
   //float fn   =   5;
   float f0   =   pow((1-fn)/(1+fn),2);
   float   EH   =   dot(nor,-viewdir);
   
   float   Fspec   =   f0+(1-f0)*pow(1-EH,5);
   float f   =   Fspec;
   return f*(fDotVR*0.5+0.5);//*float4(tex2Dlod(Texture1,float4(retUV,0,2)).xyz,fDotVR*0.5+0.5);;//fDotVR;//float4(1,1,1,fDotVR);//tex2Dlod(Texture1,float4(retUV,0,f*0.05));//+tex2D(Texture1,uv);//*WaterColor;//float4(wrefl,1);//retColor
   
}



]]></RmShaderCode>
