float fScale;
float2 invViewport;
float fLerp;
float fRoughness;
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
   float fGlossness   =   (fRoughness*fRoughness)/0.91f;

   float2 halfPixel   =   invViewport;

   float4 c  =tex2D( Texture0, texCoord);

   
   float4 cspec   =    tex2D( Texture2, texCoord+0.5f*halfPixel );

   
   float4 diff = tex2D(Texture1,texCoord+0.5f*halfPixel);
   return pow(lerp(lerp(c,cspec,fGlossness)*fScale,1,fLerp),1/fGarmma);
}

