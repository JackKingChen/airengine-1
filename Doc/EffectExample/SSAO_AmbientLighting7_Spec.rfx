     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matViewInverse;
samplerCUBE Texture0;
float fGarmma;
float4 LightDir;
float4 LightColor;


#define EPSILON 0.000001

float IntersectQuad(    float3 orig, 
                        float3 dir,
                        float3 v0, 
                        float3 v1, 
                        float3 v2,
                        out float t, 
                        out float u, 
                        out float v )
{
    // Find vectors for two edges sharing vert0
    float3 edge1 = v1 - v0;
    float3 edge2 = v2 - v0;

    // Begin calculating determinant - also used to calculate U parameter
    float3 pvec;
    pvec   =   cross( dir, edge2 );

    // If determinant is near zero, ray lies in plane of triangle
    float det = dot( edge1, pvec );

    float3 tvec;
    if( det > 0 )
    {
        tvec = orig - v0;
    }
    else
    {
        tvec = v0 - orig;
        det = -det;
    }

    if( det < 0.0001f )
       return 0;

    // Calculate U parameter and test bounds
    u = dot( tvec, pvec );
    if( u < 0.0f || u > det )
        return 0;

    // Prepare to test V parameter
    float3 qvec=cross( tvec, edge1 );

    // Calculate V parameter and test bounds
    v = dot( dir, qvec );
    if( v < 0.0f || v > det )
        return 0;

    // Calculate t, scale parameters, ray intersects triangle
    t = dot( edge2, qvec );
    
    if(t<0)
       return 0;
    
    float fInvDet = 1.0f / det;
    t *= fInvDet;
    u *= fInvDet;
    v *= fInvDet;

    return 1;
};

float4 vY;
float4 vX;
float4 vCenter;
float4 vViewPosition;

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
   float   y   =   (1-texCoord.x)*2-1;//*3.1415926f;
   float2   xz   =   float2(0,1);
   float   rotAngle   =   texCoord.y*2*3.14159265f;
   float fsin,fcos;
   sincos(rotAngle,fsin,fcos);
   float2x2 rot = float2x2(fcos,-fsin,fsin,fcos);
   float2 newxz   =   mul(xz,rot)*pow(1-(y*y),0.5);
   
   float3 xyz   =   float3(newxz.xy,y);
   
   float3 newxyz   =   normalize(mul(xyz,(float3x3)matViewInverse));
   
  // float2 uv   =   (float2(2,1))*(texCoord+float2(0.5,0))*3.14159265;
  // float3   xyz   =   float3(cos(uv.x)*sin(uv.y),cos(uv.y),sin(uv.x)*sin(uv.y));
   float4 cEnv   =    pow(texCUBElod( Texture0, float4(newxyz,3) ),1);
   //if(dot(cEnv.xyz,float3(0.3,0.6,0.1))<0.0f){
   //   discard;
   //}
   
   float3 p0   =   (vCenter - vX + vY);
   float3 p1   =   (vCenter + vX + vY);
   float3 p2   =   (vCenter + vX - vY);
   
   float t,u,v;
   
   float f   =   IntersectQuad(vViewPosition.xyz,newxyz,p1,p0,p2,t,u,v);
 #if 0
   float3 norlight   =   normalize(LightDir.xyz);
   float fd=   dot(norlight,newxyz) ;
   if(fd> 0.8){
      return LightColor*saturate((fd-0.8)*5);
   }
   if(newxyz.y<0)
       cEnv=0;//pow(1+newxyz.y,0.5);
   return 0;//cEnv;
 #else

   // if(newxyz.y<0)
    //   cEnv=0;//pow(1+newxyz.y,0.5);
   return cEnv;
 
 #endif
}

]]></RmShaderCode>
