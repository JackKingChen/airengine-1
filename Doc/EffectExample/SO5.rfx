float4 OccSize;

float4   temp   =   float4(2,-2,-1,1);

float2 GetOffset(float4 off,float4 option){
   return (off.xy*option.x+float2(1,-1)*off.yx*(1-option.x))*option.y;
};

float fNormal;
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
   float2 halfPixel   =   0.5f*invViewport;
   float2   uv=   texCoord+halfPixel;
   float2   xy=   uv*temp.xy+temp.zw;
   float4   opt   =  tex2D(Texture3,uv/invViewport/2);
   opt.y = opt.y*temp.x-temp.w;
  
   float4   oldPos;
   oldPos.xy      =   xy;
   oldPos.z       =   tex2D(Texture1,uv).x;
   oldPos.w      =   1;
   oldPos         =   mul(oldPos,matViewProjectionInverse);
   
   oldPos/=oldPos.w;
   
   float4 vNor   =   tex2D(Texture0,uv);
   clip(0.5f-vNor.w);
   float3   oldNor=   vNor.xyz;
   oldNor=oldNor*2-1;
   
   float3 viewDir   =   -normalize(vViewPosition.xyz-oldPos.xyz);
   
   oldPos.xyz+=fNormal*oldNor;
   

   //normalize(oldNor);
   //oldNor         =   normalize(mul(oldNor,(float3x3)matViewProjectionInverse));
   
   
   
 
   
   float4   tempUV;
   float2   tempXY;

   float4   tempPos   =   temp.w;
   //float3   tempDir;
   
   float occ=0;
   tempUV.zw   =   invViewport*OccSize.y;
   
   float2 UV[4];
   float4 XY[4];

   //[loop]
   for(int i=0;i<SAMPLER_NUM;i+=4){
      UV[0]     =   GetOffset(offset[i]  ,opt)*tempUV.zw+texCoord;
      UV[1]     =   GetOffset(offset[i+1],opt)*tempUV.zw+texCoord;
      UV[2]     =   GetOffset(offset[i+2],opt)*tempUV.zw+texCoord;
      UV[3]     =   GetOffset(offset[i+3],opt)*tempUV.zw+texCoord;

      XY[0].xy     =   UV[0]*temp.xy+   temp.zw;
      XY[1].xy     =   UV[1]*temp.xy+   temp.zw;
      XY[2].xy     =   UV[2]*temp.xy+   temp.zw;
      XY[3].xy     =   UV[3]*temp.xy+   temp.zw;
      
      XY[0].z      =   tex2D(Texture1,UV[0]).x;
      XY[1].z      =   tex2D(Texture1,UV[1]).x;
      XY[2].z      =   tex2D(Texture1,UV[2]).x;
      XY[3].z      =   tex2D(Texture1,UV[3]).x;
      
      XY[0].w      =   1;
      XY[1].w      =   1;
      XY[2].w      =   1;
      XY[3].w      =   1;

      XY[0]       =   mul(XY[0],matViewProjectionInverse);
      XY[1]       =   mul(XY[1],matViewProjectionInverse);
      XY[2]       =   mul(XY[2],matViewProjectionInverse);
      XY[3]       =   mul(XY[3],matViewProjectionInverse);
      
      XY[0].xyz   /=   XY[0].w;
      XY[1].xyz   /=   XY[1].w;
      XY[2].xyz   /=   XY[2].w;
      XY[3].xyz   /=   XY[3].w;
      
      
      XY[0].xyz   -=   oldPos.xyz;
      XY[1].xyz   -=   oldPos.xyz;
      XY[2].xyz   -=   oldPos.xyz;
      XY[3].xyz   -=   oldPos.xyz;
      
      
      //normalize(XY[0].xyz);//    /=   l.x;
      //normalize(XY[1].xyz);//    /=   l.y;
      //normalize(XY[2].xyz);//    /=   l.z;
     // normalize(XY[3].xyz);//    /=   l.w;
      
      float4 d;
      d.x   =   dot(viewDir.xyz,normalize(XY[0].xyz));
      d.y   =   dot(viewDir.xyz,normalize(XY[1].xyz));
      d.z   =   dot(viewDir.xyz,normalize(XY[2].xyz));
      d.w   =   dot(viewDir.xyz,normalize(XY[3].xyz));
      
      d   =   d;
      

      


      occ += dot(d,1);
      //co+=length(tempDir);
   }

  
  float   fWeight   =   lerp(dot(viewDir,oldNor)*0.25+0.75f,1,fNor);
   float fAO   =   ((occ/SAMPLER_NUM));//*fWeight;
   return fAO*0.5+0.5;//pow(acos(-fAO)/3.14f,0.5f);///4;
}

