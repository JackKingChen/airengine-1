     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D Texture3;
sampler2D Texture0;
sampler2D Texture1;
sampler3D Texture2;


float2 InvViewport;
float4 vViewPosition;
float  fWalkStep;
float  fAngle;
float4 WaterColor;
float4x4 matViewProjection;
float4x4 matViewProjectionInverse;

struct PS_INPUT 
{
   float2 TexCoord: TEXCOORD0;
   float3 Normal:   TEXCOORD1;
   float3 WorldPos: TEXCOORD2;
   float3 Tangent:  TEXCOORD3;
   float4 Proj:TEXCOORD4;
};

struct PSOUT{
   float4 Depth:COLOR0;
   float4 Diff:COLOR1;
};

float fTime0_1;
float fHeight;

float2 XY_UV(float2 xy){
   return   xy*float2(0.5,-0.5)+float2(0.5,0.5);
};
float2 UV_XY(float2 uv){
   return   uv*float2(2,-2)+float2(-1,1);
};
float fOccWeight;
float4 ps_main( float2 texCoord  : TEXCOORD0 ):COLOR
{
   float2 halfPixel   =   0.5f*InvViewport;
   float2   uv=   texCoord+halfPixel;
   float2   xy=   UV_XY(uv);

  
   float4   oldPos;
   oldPos.xy      =   xy;
   oldPos.z       =   tex2D(Texture0,uv).x;
   oldPos.w      =   1;
   oldPos         =   mul(oldPos,matViewProjectionInverse);
   oldPos/=oldPos.w;
   
   float4   vNor   =   tex2D(Texture3,uv);
   clip(0.5-vNor.w);
   float3 nor =vNor.xyz*2-1;
   
   float3 viewdir   =   normalize(oldPos.xyz- vViewPosition.xyz);
   
   float3   wrefl   =   reflect(viewdir,nor);
   
   float3   newDirWolrdPos     =  oldPos.xyz+wrefl*2;
   
   float4   newDirProjPos      =   mul(float4(newDirWolrdPos,1),matViewProjection);
   newDirProjPos/=newDirProjPos.w;
   
   float2   newDirProjUV         =   XY_UV(newDirProjPos.xy);
   
   float2   UVDir                =   normalize(newDirProjUV   -   uv)*fWalkStep;
   
   
   
   float2   randomuv         =   (uv)/64.0f/InvViewport;
   
   float   offset               =   tex3D(Texture2,float3(randomuv,fTime0_1)).x+1.5f;
   
   
   UVDir   +=   fHeight*UVDir.yx*float2(1,-1)*(tex3D(Texture2,float3(randomuv,0.5)).x*2-1);
   
   float    weight     =   1;
   float4   retColor   =   0;
   
   
   float    fDotAngle   =   dot(-viewdir,wrefl)*0.5+0.5;
   
   
   float   fInv   =   1/(1   -     fAngle);
   
   float2 retUV   =   float2(-1,-1);
   
   float f   =     10000;
   
   [loop]
   for(int i=0;i<32;i++){
   
      float2   newUV   =   uv   +  UVDir*pow(i+offset,2);
      float4   newProjPos;
      newProjPos.xy   =   UV_XY(newUV);
      newProjPos.z      =   tex2D(Texture0,newUV+halfPixel).x;
      newProjPos.w      =   1;
      
      float4   newWorldPos   =   mul(newProjPos,matViewProjectionInverse);
      newWorldPos/=newWorldPos.w;
      
      float3   newDir   =   normalize(newWorldPos.xyz-oldPos.xyz);

      float   d          =   dot(newDir,wrefl)>lerp(fAngle,0.95,fDotAngle*i/31.0f);
      if(d>0.5){
         if(weight>0.5){
            retUV      =  newUV;//tex2D(Texture1,newUV+hPixel);
            f   =   length(newWorldPos.xyz-oldPos.xyz);
            weight   =   0;
         }
      }

      
   }
   
   if(weight>0.5){
      return 1;
   }
   //
   
   return f*fOccWeight*tex2Dlod(Texture1,float4(retUV,0,f*0.05));//+tex2D(Texture1,uv);//*WaterColor;//float4(wrefl,1);//retColor
   
}



]]></RmShaderCode>
