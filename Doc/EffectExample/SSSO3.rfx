   float3 wdir      =   normalize(wrefl-fAngle*viewDir);
    float4 dirpos      =   mul(float4(oldPos.xyz+wrefl,1),matViewProjection);
    dirpos/=dirpos.w;
    
    float2   UV      =   dirpos.xy*float2(0.5,-0.5)+float2(0.5f,0.5f);
    float2   UVDir   =   invViewport*normalize(UV   -   texCoord);
   
   float2   tempUV;
   float4   tempXY;

   float4   tempPos   =   temp.w;
   
   float3   centerDir   =   0;
   //float3   tempDir;
   
   float2 occ=SAMPLER_NUM;

   

   [loop]
