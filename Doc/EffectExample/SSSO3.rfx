      tempXY.xy     =   tempUV*float2(2,-2)+   float2(-1,1);
      tempXY.z      =   tex2D(Texture1,float4(tempUV+halfPixel,0,0)).x;
      tempXY.w      =   1;
      tempXY       =   mul(tempXY,matViewProjectionInverse);
      tempXY/=tempXY.w;
 
      
      
      tempXY.xyz   -=   oldPos.xyz;

      
      
      float2 d  =   dot(wrefl.xyz,normalize(tempXY.xyz));

   
       d=saturate((d-fAngle)/(1-fAngle));
      d *=   float2(pow(saturate(1-length(tempXY.xyz)/fDisWeight),2),(fScale/length(tempXY.xyz)));

      


