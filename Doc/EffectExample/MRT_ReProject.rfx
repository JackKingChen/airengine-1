     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float fBump;
sampler2D Texture3;
float4x4 matViewProjectionInverse;
sampler2D Texture1;
struct VS_OUTPUT
{
   float4 pos       : POSITION0;
   float2 texCoord  : TEXCOORD0;
   float4 color     : TEXCOORD1;
   
};
float4x4 matViewProjection_1;
float2 fInvViewport;
float4 vViewPos;
VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT o = (VS_OUTPUT) 0;



   o.pos = float4( inPos.xy*2-1, 0.0f, 1.0f);

   // get into range [0,1]
   o.texCoord = (float2(o.pos.x, -o.pos.y) + 1.0f)/2.0f;
   float invWH   =   1.0f/256.0f;
   float2 uv   =   o.texCoord+0.5f*invWH;
   float z   =tex2Dlod(Texture1,float4(uv,0,0)).x;
   float4 vNormal   =tex2Dlod(Texture3,float4(uv,0,0));//*2-1;
   vNormal.xyz   =   vNormal.xyz*2-1;
   //float dd   =   ddx(z)+ddy(z);
   
   

   

   
   o.pos.z    =   z;
   float4 wpos   =   mul(o.pos,matViewProjectionInverse);
   wpos/=wpos.w;
   
   float3 viewdir   =   normalize(vViewPos.xyz-wpos.xyz);
   float fdot   =   dot(viewdir,vNormal.xyz);
   
   
   wpos.xyz+=vNormal.xyz*vNormal.w*fBump;//*saturate(fdot*2);
   float4 proj   =   mul(wpos,matViewProjection_1);
   o.pos   =   proj;
   o.color       =   proj;
   return o;
}
]]></RmShaderCode>
