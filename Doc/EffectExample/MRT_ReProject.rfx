sampler2D baseMap;

struct PS_INPUT 
{
   
   float2 Texcoord : TEXCOORD0;
   float3  Normal:TEXCOORD1;
   float4   ProjPos:TEXCOORD2;
};
struct PS_OUTPUT{
   float4   Depth   :   COLOR0;
   float4   Normal   :   COLOR1;
   float4   Diffuse   :   COLOR2;
};
PS_OUTPUT ps_main( PS_INPUT Input )
{
   PS_OUTPUT   O;
   O.Depth    =   Input.ProjPos.z/Input.ProjPos.w;
   O.Normal   =   float4(Input.Normal.xyz*0.5+0.5,0);
   O.Diffuse  =   tex2D( baseMap, Input.Texcoord );
   O.Normal.w = O.Diffuse.x;
   return O;
   
}



