     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D Texture0;
float2 fInvViewport;
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
   float z    =tex2Dlod(Texture0,float4(texCoord,0,0)).x;
   float z1   =tex2Dlod(Texture0,float4(texCoord+fInvViewport*float2(1,0),0,0)).x;
   float z2   =tex2Dlod(Texture0,float4(texCoord+fInvViewport*float2(-1,0),0,0)).x;
   float z3   =tex2Dlod(Texture0,float4(texCoord+fInvViewport*float2(0,1),0,0)).x;
   float z4   =tex2Dlod(Texture0,float4(texCoord+fInvViewport*float2(0,-1),0,0)).x;
      float zoffset   =   0;
   zoffset   +=   abs(z-z1);
   zoffset   +=   abs(z-z2);
   zoffset   +=   abs(z-z3);
   zoffset   +=   abs(z-z4);
   
   if(zoffset*1000   >   0.1){
      discard;
   }
   if(z > 0.9995f){
      discard;
   }
   return 1;//(ddx(z)+ddy(z))*10000;
}

